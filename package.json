  "version": "0.0.1",
  "engines": {
    "vscode": "^1.51.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:jinc.inProgressIssues",
    "onCommand:jinc.setup"
  ],
  "main": "./dist/main.js",
  "contributes": {
    "commands": [
      {
        "command": "jinc.setup",
        "title": "JINC setup"
      },
      {
        "command": "jinc.inProgressIssues",
        "title": "Find assigned issues"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js",
